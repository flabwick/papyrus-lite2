# React Development Server Configuration for Production VM
# This allows the React dev server to accept connections from nginx proxy
DANGEROUSLY_DISABLE_HOST_CHECK=true

# Port configuration for nginx mapping
PORT=4201

# Backend API URL (proxied through nginx)
REACT_APP_API_URL=https://api-dev.jimboslice.xyz

# Disable source maps in production for better performance
GENERATE_SOURCEMAP=false
